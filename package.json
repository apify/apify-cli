{
    "name": "apify-cli",
    "version": "0.16.1",
    "description": "Apify command-line interface helps you create, develop, build and run Apify actors, and manage the Apify cloud platform.",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 180000 --recursive",
        "test-python": "mocha --timeout 180000 --recursive --grep '\\[python\\]'",
        "lint": "eslint src test",
        "lint:fix": "eslint src test --fix",
        "commands-md": "npm run manifest && oclif-dev readme",
        "prepare-release": "npm run commands-md && npm run prod-shrinkwrap",
        "prod-shrinkwrap": "rm -rf node_modules && npm install --production && npm shrinkwrap",
        "manifest": "rm -f oclif.manifest.json && oclif-dev manifest",
        "postinstall": "node \"./src/bin/run\" check-version"
    },
    "files": [
        "src",
        "oclif.manifest.json",
        "index.js",
        "package.json"
    ],
    "bin": {
        "apify": "./src/bin/run"
    },
    "contributors": [
        "Jakub Drobn√≠k <jakub.drobnik@apify.com>",
        "Jan Curn <jan@apify.com>"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apify/apify-cli.git"
    },
    "keywords": [
        "apify",
        "client",
        "node",
        "command",
        "line",
        "bash"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
        "url": "https://www.apify.com"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/apify/apify-cli/issues"
    },
    "homepage": "https://github.com/apify/apify-cli#readme",
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "@apify/actor-templates": "^0.1.3",
        "@apify/consts": "^2.7.0",
        "@apify/input_schema": "^3.3.0",
        "@apify/utilities": "^2.2.3",
        "@crawlee/memory-storage": "^3.0.3",
        "@oclif/command": "^1.8.16",
        "@oclif/config": "^1.18.3",
        "@oclif/errors": "^1.3.5",
        "@oclif/plugin-commands": "^2.1.0",
        "@oclif/plugin-help": "^5.1.12",
        "adm-zip": "^0.5.10",
        "ajv": "^8.10.0",
        "apify-client": "^2.2.0",
        "archiver-promise": "^1.0.0",
        "axios": "^0.21.1",
        "chalk": "^4.1.2",
        "computer-name": "^0.1.0",
        "cors": "^2.8.5",
        "detect-indent": "^6.1.0",
        "escape-string-regexp": "^4.0.0",
        "express": "^4.18.2",
        "globby": "^11.1.0",
        "inquirer": "^7.3.3",
        "is-online": "^10.0.0",
        "istextorbinary": "^6.0.0",
        "load-json-file": "^6.2.0",
        "mime": "^2.6.0",
        "open": "^8.4.0",
        "ow": "^0.28.1",
        "rimraf": "^3.0.2",
        "semver": "^7.3.5",
        "tiged": "^2.12.4",
        "underscore": "^1.13.2",
        "write-json-file": "^4.3.0"
    },
    "devDependencies": {
        "@apify/eslint-config": "^0.2.3",
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^2.1.0",
        "chai": "^4.3.4",
        "chai-match": "^1.1.1",
        "eslint": "^8.11.0",
        "mocha": "^10.0.0",
        "sinon": "^9.2.4"
    },
    "oclif": {
        "bin": "apify",
        "commands": "./src/commands",
        "hooks": {
            "init": [
                "./src/hooks/init"
            ]
        }
    }
}
