{
    "name": "apify-cli",
    "version": "0.7.1",
    "description": "Apify command-line interface helps you create, develop, build and run Apify actors, and manage the Apify cloud platform.",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 180000 --recursive",
        "lint": "eslint src test",
        "lint:fix": "eslint src test --fix",
        "commands-md": "npm run manifest && oclif-dev readme",
        "prod-shrinkwrap": "rm -rf node_modules && npm install --production && npm shrinkwrap",
        "manifest": "rm -f oclif.manifest.json && oclif-dev manifest"
    },
    "files": [
        "src",
        "npm-shrinkwrap.json",
        "oclif.manifest.json",
        "index.js",
        "package.json"
    ],
    "bin": {
        "apify": "./src/bin/run"
    },
    "contributors": [
        "Jakub Drobn√≠k <jakub.drobnik@apify.com>",
        "Jan Curn <jan@apify.com>"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/apify/apify-cli.git"
    },
    "keywords": [
        "apify",
        "client",
        "node",
        "command",
        "line",
        "bash"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
        "url": "https://www.apify.com"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/apify/apify-cli/issues"
    },
    "homepage": "https://github.com/apify/apify-cli#readme",
    "engines": {
        "node": ">=12.0.0"
    },
    "dependencies": {
        "@apify/actor-templates": "^0.1.1",
        "@apify/consts": "^1.7.0",
        "@apify/http-request": "^2.1.2",
        "@apify/input_schema": "^2.0.8",
        "@apify/storage-local": "^2.0.2",
        "@oclif/command": "^1.8.16",
        "@oclif/config": "^1.18.3",
        "@oclif/errors": "^1.3.5",
        "@oclif/plugin-commands": "^2.1.0",
        "@oclif/plugin-help": "^5.1.12",
        "ajv": "^8.10.0",
        "apify-client": "^2.2.0",
        "archiver-promise": "^1.0.0",
        "chalk": "^4.1.2",
        "child-process-promise": "^2.2.1",
        "extract-zip": "^2.0.1",
        "globby": "^11.1.0",
        "inquirer": "^7.3.3",
        "is-online": "^9.0.1",
        "load-json-file": "^6.2.0",
        "mime": "^2.6.0",
        "minimist": "^1.2.5",
        "ow": "^0.28.1",
        "rimraf": "^3.0.2",
        "semver": "^7.3.5",
        "underscore": "^1.13.2",
        "unzipper": "^0.10.11",
        "write-json-file": "^4.3.0"
    },
    "devDependencies": {
        "@apify/eslint-config": "^0.2.3",
        "@oclif/dev-cli": "^1.26.0",
        "@oclif/test": "^2.1.0",
        "chai": "^4.3.4",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.11.0",
        "mocha": "^8.4.0",
        "sinon": "^9.2.4"
    },
    "eslintIgnore": [
        "src/templates/*"
    ],
    "oclif": {
        "bin": "apify",
        "commands": "./src/commands",
        "hooks": {
            "prerun": [
                "./src/hooks/pre_run.js"
            ]
        }
    }
}
